<Window x:Class="MebelCourseWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MebelCourseWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300" >
    
    <Window.Background>
        <ImageBrush ImageSource="Images/tree_back.jpg"></ImageBrush>
    </Window.Background>
    
    <StackPanel>
        <Label FontSize="22" FontWeight="Bold" HorizontalAlignment="Center" Margin="30" Foreground="#513C2F">Введите пароль</Label>
        <PasswordBox x:Name="enterText" Width="200" Height="25" Background="#FDD48A" BorderBrush="#513C2F" Foreground="#513C2F" FontSize="12"></PasswordBox>
        <Button Height="50" Width="200" FontSize="22" FontWeight="Bold" Margin="40" Click="Enter" Background="#83D350" Foreground="#513C2F" BorderBrush="#513C2F">Войти</Button>
    </StackPanel>
</Window>
